{
  "expo": {
    "name": "PackTracker",
    "slug": "packtrackerapp",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/pt_logo_1.png",
    "splash": {
      "image": "./assets/images/pt_logo_1.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.evanrose.dogapp"
    },
    "android": {
      "package": "com.evanrose.dogapp"
    },
    "androidStatusBar": {
      "backgroundColor": "#ffae00"
    }
  }
}